<ol>
    <% items.forEach(list => { %>
        <li>
            <% if (list.type === "folder") { %>
                <strong><%= list.title %></strong>
                <%- include('listTree', { items: list.children }) %>
            <% } else if (list.type === "list") { %>
                <a href="/list/<%= list.id %>/"><%= list.title %></a>
            <% } %>
        </li>
    <% }) %>
</ol>